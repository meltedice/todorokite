{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
    "plugin:flowtype/recommended",
    "plugin:prettier/recommended",
    "prettier",
    "prettier/flowtype",
    "prettier/react",
    "prettier/standard"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "react/prop-types": "off",
    "react/no-unused-prop-types": "off"
  }
}
